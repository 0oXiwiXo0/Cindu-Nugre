<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <link rel="icon" href="{{ asset('images/cindy.svg') }}">

    {% block stylesheets %}
        <!-- UIkit CSS -->
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/uikit@3.23.12/dist/css/uikit.min.css" />
        <link rel="stylesheet" href="{{ asset('styles/style.css') }}">
    {% endblock %}

    {% block javascripts %}
        <!-- UIkit JS -->
        <script src="https://cdn.jsdelivr.net/npm/uikit@3.23.12/dist/js/uikit.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/uikit@3.23.12/dist/js/uikit-icons.min.js"></script>
    {% endblock %}
</head>
<body>
<div class="page-container">

    <div class="logo-photographer">
        <img src="{{ asset('images/logo_Cindy.png') }}" alt="Logo Cindy" class="logo-header">
    </div>

    <header>
        {% include('main/navbar.html.twig') %}
    </header>

    <main class="content">
        {% block body %}{% endblock %}
    </main>

    <footer class="footer">
        <div class="container">
            &copy; Cindy Nugre {{ "now"|date("Y") }}
        </div>
    </footer>
</div>
</body>
</html>
